<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="HandicapMobile.Views.RegisterClubPage">
    <ContentPage.Content>
        <StackLayout>
            <Entry x:Name="txtClubName" Placeholder="Enter Club Name" Text="{Binding ClubName}" ></Entry>
            <Button x:Name="btnRegisterClub" Text="Register Club" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>